{
  "version": 2,
  "builds": [
    {
      "src": "kei.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "300mb",
        "runtime": "python3"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "kei.py"
    }
  ],
  "buildCommand": "pip install -r requirements.txt && python -m flask db upgrade",
}
